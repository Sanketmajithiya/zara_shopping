num = 9
for row in range(1, num+1):
    for col in range(1, num+1):
        flag = num//2
        middle = (flag + 1)
        if (col == 1 and row <= flag) or \
            (row >= (flag + 1) and col == num) or \
            (col >= (flag + 1) and row == 1) or \
            (col <= (flag + 1) and row == num) or \
            (row == middle or col == middle):
            print("*", end=' ')
            continue
        else:
            print(" ", end=' ')
    print()